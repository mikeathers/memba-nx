{
  "name": "@memba-nx/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build --parallel=4",
    "test": "nx run-many --target=test --all --parallel=4",
    "lint": "yarn lint:workspace && yarn lint:eslint --parallel=4 && yarn stylelint --parallel=4",
    "lint:eslint": "nx run-many --all --target=lint",
    "lint:workspace": "nx workspace-lint",
    "stylelint": "nx run-many --all --target stylelint",
    "stylelint:fix": "nx run-many --all --target stylelint --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "list-affected": "nx print-affected --select=projects",
    "prepare": "husky install",
    "affected": "yarn affected:test && yarn affected:lint",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "print-affected:libs": "nx print-affected --select=projects --type=lib",
    "print-affected:apps": "nx print-affected --select=projects --type=app",
    "affected:build:libs": "bash ./tools/scripts/build-libs.sh",
    "affected:build:apps": "AFFECTED_PROJECTS=\"$(yarn run --silent print-affected:apps --target=build)\"; if [-z \"$AFFECTED_PROJECTS\"]; then nx affected --target=build; else nx run-many --target=build --projects=\"$AFFECTED_PROJECTS\"; fi",
    "clean": "rimraf node_modules dist coverage tmp",
    "ts-node": "ts-node"
  },
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^5.6.4",
    "@aws-sdk/client-cloudfront": "^3.405.0",
    "@aws-sdk/credential-providers": "^3.405.0",

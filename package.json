    "@nx/rollup": "16.7.4",
    "@nx/workspace": "16.7.1",
    "@rollup/plugin-url": "^7.0.0",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@swc/jest": "^0.2.29",
    "@swc/plugin-styled-components": "^1.5.67",
    "@testing-library/jest-dom": "^6.1.2",

    "@swc/helpers": "~0.5.0",
    "formik": "^2.4.3",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^14.0.1",
    "next": "13.4.1",
    "open-next": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "recast": "^0.23.4",
    "styled-components": "5.3.6",
    "tslib": "^2.3.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.22.11",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nx/cypress": "16.7.1",
    "@nx/devkit": "^16.7.1",
    "@nx/eslint-plugin": "16.7.1",
    "@nx/jest": "16.7.1",
    "@nx/js": "16.7.4",
    "@nx/linter": "16.7.1",
    "@nx/next": "16.7.4",

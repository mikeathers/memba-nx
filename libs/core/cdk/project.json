{
  "name": "core-cdk",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/cdk/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/core/cdk",
        "main": "libs/core/cdk/src/index.ts",
        "tsConfig": "libs/core/cdk/tsconfig.lib.json",
        "assets": [
          "libs/core/cdk/*.md",
          {
            "input": "./libs/core/cdk/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/core/cdk/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/core/cdk",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/core/cdk",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/core/cdk/**/*.ts",
          "libs/core/cdk/generators.json",
          "libs/core/cdk/executors.json",
          "libs/core/cdk/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/core/cdk"
      ],
      "options": {
        "jestConfig": "libs/core/cdk/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "type:plugin",
    "domain:core",
    "scope:shared-internally"
  ]
}
